{
  "name": "betterspotify",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron.js",
  "dependencies": {
    "@craco/craco": "^6.1.2",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^12.8.3",
    "axios": "^0.21.1",
    "electron-is-dev": "^2.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "spotify-web-api-node": "^5.0.2",
    "web-vitals": "^1.1.1"
  },
  "scripts": {
    "start": "set PORT=8082 && craco start ",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "set PORT=8082 && set BROWSER=none && concurrently -k \" npm start\" \"npm:electron\"",
    "electron": "wait-on tcp:8082 && electron .",
    "build-css": "tailwindcss build src/styles/tailwind.css -o src/styles/main.css"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^9.8.6",
    "concurrently": "^6.0.2",
    "electron": "^12.0.6",
    "postcss": "^7.0.35",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.1.2",
    "wait-on": "^5.3.0",
    "webpack-cli": "^4.6.0"
  }
}
